{
  "manifest_version": 3,
  "name": "Auto Form Filler",
  "description": "Automatically fills forms with predefined data.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {

    }
  }
}

